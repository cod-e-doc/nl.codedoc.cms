{
  "kind": "singleType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contacts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "phone": {
      "type": "string",
      "regex": "^\\+[1-9]\\d{1,14}$"
    },
    "eMail": {
      "type": "email"
    },
    "address": {
      "type": "string",
      "maxLength": 255
    },
    "linkedInProfile": {
      "type": "string",
      "regex": "^[a-zA-Z0-9][a-zA-Z0-9_-]{2,99}$"
    }
  }
}
